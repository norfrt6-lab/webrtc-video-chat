{
  "name": "webrtc-video-chat",
  "version": "2.0.0",
  "description": "Real-time video conferencing with WebRTC, Socket.io, and advanced collaboration features",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"cd client && npm run dev\"",
    "dev:server": "nodemon server.js",
    "dev:client": "cd client && npm run dev",
    "build:client": "cd client && npm run build",
    "test": "cd client && npm test",
    "postinstall": "cd client && npm install"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
